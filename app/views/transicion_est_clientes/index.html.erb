<h1>Listing transicion_est_clientes</h1>

<table>
  <thead>
    <tr>
      <th>Est ini cliente</th>
      <th>Est fin cliente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transicion_est_clientes.each do |transicion_est_cliente| %>
      <tr>
        <td><%= transicion_est_cliente.est_ini_cliente %></td>
        <td><%= transicion_est_cliente.est_fin_cliente %></td>
        <td><%= link_to 'Show', transicion_est_cliente %></td>

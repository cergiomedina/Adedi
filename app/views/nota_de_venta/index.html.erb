<h1>Listing nota_de_venta</h1>

<table>
  <thead>
    <tr>
      <th>Id nota</th>
      <th>Total nota</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nota_de_venta.each do |nota_de_ventum| %>
      <tr>
        <td><%= nota_de_ventum.id_nota %></td>
        <td><%= nota_de_ventum.total_nota %></td>
        <td><%= link_to 'Show', nota_de_ventum %></td>
        <td><%= link_to 'Edit', edit_nota_de_ventum_path(nota_de_ventum) %></td>
        <td><%= link_to 'Destroy', nota_de_ventum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nota de ventum', new_nota_de_ventum_path %>

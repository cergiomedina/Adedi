<h1>Listing ejemplar_estados</h1>

<table>
  <thead>
    <tr>
      <th>Id estado cliente</th>
      <th>Est ejemplar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ejemplar_estados.each do |ejemplar_estado| %>
      <tr>
        <td><%= ejemplar_estado.id_estado_cliente %></td>
        <td><%= ejemplar_estado.est_ejemplar %></td>
        <td><%= link_to 'Show', ejemplar_estado %></td>
        <td><%= link_to 'Edit', edit_ejemplar_estado_path(ejemplar_estado) %></td>
        <td><%= link_to 'Destroy', ejemplar_estado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ejemplar estado', new_ejemplar_estado_path %>
